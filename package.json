{
  "name": "greatdebate",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production next start -p ${PORT:-3000}",
    "lint": "next lint",
    "api": "node server/index.js",
    "dev:api": "nodemon server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run api\"",
    "test:prod": "npm run build && npm run start",
    "prod:all": "concurrently \"npm run start\" \"npm run api\"",
    "mvp": "concurrently \"npm run api\" \"npm run dev\"",
    "clean:next": "rm -rf .next || true",
    "reset": "bash scripts/reset-cache.sh",
    "nuclear": "bash scripts/reset-cache.sh && npm run mvp",
    "nuclear:api": "node server/index.js",
    "nuclear:next": "next dev",
    "mvp:silent": "concurrently \"npm run nuclear:api\" \"npm run nuclear:next\"",
    "test": "vitest",
    "test:api": "node server/server-test.js",
    "test:component": "node tests/component-request.test.js",
    "test:api:stress": "node server/server-test.js --stress",
    "test:all": "npm run test:api && npm run test:component",
    "test:quick": "node server/quick-test.js",
    "test:watch": "vitest watch",
    "generate-test-pdf": "node scripts/generate-test-pdf.js",
    "worker": "npx tsx src/lib/queue/worker.ts",
    "postinstall": "prisma generate"
  },
